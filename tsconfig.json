{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules",
        "typings/browser",
        "typings/main",
        "typings/main.d.ts",
        "jspm_packages"
    ]
}
